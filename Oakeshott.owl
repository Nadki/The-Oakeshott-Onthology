                                                                                                  [ rdf:type owl:Restriction ;
                                                                                                    owl:onProperty oto:composedOf ;
                                                                                                    owl:someValuesFrom [ owl:intersectionOf ( oto:Blade
                                                                                                                                              [ rdf:type owl:Class ;
                                                                                                                                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                                                                                                owl:onProperty oto:length ;
                                                                                                                                                                owl:hasValue 91
                                                                                                                                                              ]
                                                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                                                owl:onProperty oto:length ;
                                                                                                                                                                owl:hasValue 92
                                                                                                                                                              ]
                                                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                                                owl:onProperty oto:length ;
                                                                                                                                                                owl:hasValue 93
                                                                                                                                                              ]
                                                                                                                                                            )
                                                                                                                                              ]
                                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                                owl:onProperty oto:hasCrossSection ;
                                                                                                                                                owl:hasValue oto:Lenticular
                                                                                                                                              ]
                                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                                owl:onProperty oto:hasFuller ;
