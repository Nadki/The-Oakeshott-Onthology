                                                                                                                       ]
                                                                                                  ]
                                                                                                  [ rdf:type owl:Restriction ;
                                                                                                    owl:onProperty oto:composedOf ;
                                                                                                    owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                                         owl:oneOf ( oto:A
                                                                                                                                     oto:B
                                                                                                                                     oto:G
                                                                                                                                     oto:H
                                                                                                                                     oto:I
                                                                                                                                     oto:J
                                                                                                                                     oto:K
                                                                                                                                     oto:M
                                                                                                                                   )
                                                                                                                       ]
