#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/oakeshott#composedOf
oto:composedOf rdf:type owl:ObjectProperty ;
               owl:inverseOf oto:partOf ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range owl:Thing ;
               rdfs:comment "The relation between an object and its components"@en ;
               rdfs:label "composed of"@en .


###  http://www.semanticweb.org/oakeshott#hasBladeSection
oto:hasBladeSection rdf:type owl:ObjectProperty ;
                    rdfs:domain oto:MedievalSword ;
                    rdfs:range oto:CrossSection ;
                    owl:propertyChainAxiom ( oto:composedOf
                                             oto:hasCrossSection
                                           ) ;
                    rdfs:comment "The relation between the sword and the cross section of its blade"@en ;
                    rdfs:label "has blade section"@en .


###  http://www.semanticweb.org/oakeshott#hasCrossSection
oto:hasCrossSection rdf:type owl:ObjectProperty ;
                    rdfs:domain oto:Blade ;
                    rdfs:range oto:CrossSection ;
                    rdfs:comment "The relation between the blade and its cross section"@en ;
                    rdfs:label "has cross section"@en .


###  http://www.semanticweb.org/oakeshott#hasFuller
oto:hasFuller rdf:type owl:ObjectProperty ;
              rdfs:domain oto:Blade ;
              rdfs:range oto:Fuller ;
              rdfs:comment "The relation between the blade and its fuller"@en ;
              rdfs:label "has fuller"@en .


###  http://www.semanticweb.org/oakeshott#hasType
oto:hasType rdf:type owl:ObjectProperty ;
            owl:inverseOf oto:typeOf ;
            rdfs:domain oto:MedievalSword ;
            rdfs:range oto:SwordType ;
            rdfs:comment "The relation between the sword and its type"@en ;
            rdfs:label "has type"@en .


###  http://www.semanticweb.org/oakeshott#isFrom
oto:isFrom rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain oto:MedievalSword ;
           rdfs:range oto:Country ;
           rdfs:comment "The relation between the sword and its country of origin"@en ;
           rdfs:label "is from"@en .


###  http://www.semanticweb.org/oakeshott#partOf
oto:partOf rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain owl:Thing ;
           rdfs:range owl:Thing ;
           rdfs:comment "The relation between a part and an object composed by it"@en ;
           rdfs:label "part of"@en .


###  http://www.semanticweb.org/oakeshott#typeOf
oto:typeOf rdf:type owl:ObjectProperty ;
           rdfs:domain oto:SwordType ;
           rdfs:range oto:MedievalSword ;
           rdfs:comment "The relation between the type and a sword which has it"@en ;
           rdfs:label "type of"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/oakeshott#gripType
oto:gripType rdf:type owl:DatatypeProperty ;
             rdfs:domain oto:Hilt ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:onDatatype xsd:integer ;
                          owl:withRestrictions ( [ xsd:minInclusive 1
                                                 ]
                                                 [ xsd:maxInclusive 2
                                                 ]
                                               )
                        ] ;
             rdfs:comment "The grip type of the hilt"@en ;
             rdfs:label "grip type"@en .


###  http://www.semanticweb.org/oakeshott#image
oto:image rdf:type owl:DatatypeProperty ;
          rdfs:domain oto:MedievalSword ;
          rdfs:range xsd:string ;
          rdfs:comment "The URL of the image of the sword"@en ;
          rdfs:label "image"@en .


###  http://www.semanticweb.org/oakeshott#length
oto:length rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain oto:Blade ;
           rdfs:range xsd:positiveInteger ;
           rdfs:comment "The length of the blade"@en ;
           rdfs:label "length"@en .


###  http://www.semanticweb.org/oakeshott#name
oto:name rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain oto:MedievalSword ;
         rdfs:range xsd:string ;
         rdfs:comment "The name of the sword"@en ;
         rdfs:label "name"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/oakeshott#Blade
oto:Blade rdf:type owl:Class ;
          rdfs:comment "The blade of the sword"@en ;
          rdfs:label "Blade"@en .


###  http://www.semanticweb.org/oakeshott#Country
oto:Country rdf:type owl:Class ;
            rdfs:comment "Countries of the world"@en ;
            rdfs:label "Country"@en ;
            <http://www.w3.org/2004/02/skos/core#closeMatch> <https://www.wikidata.org/entity/Q3624078> .


###  http://www.semanticweb.org/oakeshott#CrossSection
oto:CrossSection rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( oto:Diamond
                                                   oto:Hexagonal
                                                   oto:Hollow-Ground
                                                   oto:Lenticular
                                                 )
                                     ] ;
                 rdfs:comment "The cross section of the blade"@en ;
                 rdfs:label "Cross Section"@en .


###  http://www.semanticweb.org/oakeshott#Fuller
oto:Fuller rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( oto:Multiple
                                             oto:Narrow
                                             oto:None
                                             oto:Wide
                                           )
                               ] ;
           rdfs:comment "The fuller of the sword"@en ;
           rdfs:label "Fuller"@en .


###  http://www.semanticweb.org/oakeshott#Guard
oto:Guard rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( oto:Guard01
                                            oto:Guard02
                                            oto:Guard03
                                            oto:Guard04
                                            oto:Guard05
                                            oto:Guard06
                                            oto:Guard07
                                            oto:Guard08
                                            oto:Guard09
                                            oto:Guard10
                                            oto:Guard11
                                            oto:Guard12
                                          )
                              ] ;
          rdfs:comment "The guard of the sword"@en ;
          rdfs:label "Guard"@en .


###  http://www.semanticweb.org/oakeshott#Hilt
oto:Hilt rdf:type owl:Class ;
         rdfs:comment "The hilt of the sword"@en ;
         rdfs:label "Hilt"@en .


###  http://www.semanticweb.org/oakeshott#MedievalSword
oto:MedievalSword rdf:type owl:Class ;
                  rdfs:comment "A sword in the style of those from the middle ages"@en ;
                  rdfs:label "Medieval Sword"@en ;
                  <http://www.w3.org/2004/02/skos/core#narrower> <http://vocab.getty.edu/page/aat/300037048> .


###  http://www.semanticweb.org/oakeshott#OneHanded
oto:OneHanded rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty oto:typeOf ;
                                    owl:someValuesFrom [ owl:intersectionOf ( oto:MedievalSword
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty oto:composedOf ;
                                                                                owl:someValuesFrom [ owl:intersectionOf ( oto:Hilt
                                                                                                                          [ rdf:type owl:Restriction ;
                                                                                                                            owl:onProperty oto:gripType ;
                                                                                                                            owl:hasValue 1
                                                                                                                          ]
                                                                                                                        ) ;
                                                                                                     rdf:type owl:Class
                                                                                                   ]
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ;
              rdfs:subClassOf oto:SwordType ;
              rdfs:comment "Types of swords that are handled with one hand"@en ;
              rdfs:label "One Handed"@en .


###  http://www.semanticweb.org/oakeshott#Pommel
oto:Pommel rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( oto:A
                                             oto:B
                                             oto:B1
                                             oto:C
                                             oto:D
                                             oto:E
                                             oto:F
                                             oto:G
                                             oto:G1
                                             oto:G2
                                             oto:H
                                             oto:H1
                                             oto:I
                                             oto:I1
                                             oto:J
                                             oto:K
                                             oto:L
                                             oto:M
                                             oto:N
                                             oto:O
                                             oto:P
                                             oto:Q
                                             oto:R
                                             oto:S
                                             oto:T
                                             oto:T1
                                             oto:T2
                                             oto:T3
                                             oto:T4
                                             oto:T5
                                             oto:U
                                             oto:V
                                             oto:V1
                                             oto:V2
                                             oto:W
                                             oto:Z
                                           )
                               ] ;
           rdfs:comment "The pommel of the sword"@en ;
           rdfs:label "Pommel"@en .


###  http://www.semanticweb.org/oakeshott#SwordType
oto:SwordType rdf:type owl:Class ;
              rdfs:comment "The type of the sword"@en ;
              rdfs:label "Sword Type"@en .


###  http://www.semanticweb.org/oakeshott#TwoHanded
oto:TwoHanded rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty oto:typeOf ;
                                    owl:someValuesFrom [ owl:intersectionOf ( oto:MedievalSword
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty oto:composedOf ;
                                                                                owl:someValuesFrom [ owl:intersectionOf ( oto:Hilt
                                                                                                                          [ rdf:type owl:Restriction ;
                                                                                                                            owl:onProperty oto:gripType ;
                                                                                                                            owl:hasValue 2
                                                                                                                          ]
                                                                                                                        ) ;
                                                                                                     rdf:type owl:Class
                                                                                                   ]
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ;
              rdfs:subClassOf oto:SwordType ;
              rdfs:comment "Types of swords that are handled with two hands"@en ;
              rdfs:label "Two Handed"@en .


